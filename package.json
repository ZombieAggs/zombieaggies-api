{
    "name": "zombieaggies-api",
    "version": "1.1.1",
    "private": true,
    "scripts": {
        "build": "tsc && node ./node_modules/gulp/bin/gulp.js build",
        "build:watch": "nodemon --watch src --exec \"npm run build\"",
        "start": " node ./build/src/server/index.js",
        "test": "node ./node_modules/mocha/bin/mocha.js --exit --recursive",
        "start:watch": "nodemon --watch build --exec \"node ./build/src/server/index.js\" --delay 4.0",
        "remigrate": "node ./node_modules/knex/bin/cli.js migrate:rollback && node ./node_modules/knex/bin/cli.js migrate:latest && knex seed:run",
        "heroku-postbuild": "node ./node_modules/knex/bin/cli.js migrate:latest && node ./node_modules/typescript/bin/tsc && node ./node_modules/gulp/bin/gulp.js build"
    },
